  "keywords": [
    "css",
    "libsass",
    "preprocessor",
    "sass",
    "scss",
    "style"
  ],
  "dependencies": {
    "chalk": "^0.5.1",
    "cross-spawn": "^0.2.3",
    "gaze": "^0.5.1",
    "get-stdin": "^3.0.0",
    "meow": "^2.0.0",
    "mkdirp": "^0.5.0",
    "mocha": "^2.0.1",
    "nan": "^1.5.1",
    "npmconf": "^2.1.1",
    "object-assign": "^2.0.0",
    "replace-ext": "0.0.1",
    "request": "^2.48.0",
    "sass-graph": "^1.0.1",
    "shelljs": "^0.3.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.1",
    "jscoverage": "^0.5.6",
    "jshint": "^2.5.5",
    "mocha-lcov-reporter": "^0.0.1"
  }
